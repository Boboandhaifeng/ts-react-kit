{
    "compilerOptions": {
      "target": "es6",
      "module": "ESNext",
      "sourceMap": true,
      "noImplicitAny": true,
      "removeComments": false,
      "allowJs": true,
      "allowSyntheticDefaultImports": true,
      "moduleResolution": "node",
      "jsx": "react",
      "baseUrl": "./",
      "paths": {
        "components/*": ["src/components/*"],
        "consts": ["src/consts"],
        "utils/*": ["src/utils/*"],
        "store": ["src/store"],
        "images/*": ["src/images/*"]
      }
    }
  }
  